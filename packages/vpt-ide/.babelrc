{
    "presets": ["es2015", "react", "stage-0", "env"],
    "plugins": ["react-hot-loader/babel", ["import", {
            "libraryName": "antd",
            "style": true
        }],
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }], "transform-class-properties"
    ],
    "env": {
        "production": {
            "preset": ["react-optimize"]
        }
    }
}